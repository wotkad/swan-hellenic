extends ../layouts/master

block title
  title Swan Hellenic | Privacy Policy

block basicSeo
  meta(content="This page Privacy Policy" name="description")
  meta(content="Page, Privacy Policy" name="keywords")

block manifestBrowserconfigFiles
  link(href="../manifest.json" rel="manifest" crossorigin)
  meta(content="../browserconfig.xml" name="msapplication-config")

block content
  main.section-not-green
    include ../mixins/breadcrumbs
    include ../mixins/heading
    section.gapper.section.section-white.section-has-sticky
      .wrapper
        +breadcrumbs([{type: 'span', title: 'Privacy Policy'}])
        +heading('Privacy Policy', '', 'privacy-policy', 'heading__icon-privacy-policy')
        include ../components/privacy-policy