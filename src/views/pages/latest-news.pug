extends ../layouts/master

block title
  title Swan Hellenic | Latest News

block basicSeo
  meta(content="This page Latest News" name="description")
  meta(content="Page, Latest News" name="keywords")

block manifestBrowserconfigFiles
  link(href="../manifest.json" rel="manifest" crossorigin)
  meta(content="../browserconfig.xml" name="msapplication-config")

block content
  main.section-not-green
    include ../mixins/breadcrumbs
    include ../mixins/heading
    include ../mixins/button
    section.gapper.section.section-white
      .wrapper-xl
        .wrapper
          +breadcrumbs([ {type: 'span', title: 'Latest News'} ])
          +heading('Latest News', '', '', '')
          include ../components/tags.pug
          include ../components/latest-news.pug