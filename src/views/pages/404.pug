extends ../layouts/master

block title
  title Swan Hellenic | 404

block basicSeo
  meta(content="This page 404" name="description")
  meta(content="Page, 404" name="keywords")

block manifestBrowserconfigFiles
  link(href="../manifest.json" rel="manifest" crossorigin)
  meta(content="../browserconfig.xml" name="msapplication-config")

block content
  main.section-not-green
    include ../mixins/breadcrumbs
    include ../mixins/heading
    include ../mixins/default-slider
    section.gapper.section.section-white
      .wrapper
        +breadcrumbs([{type: 'span', title: '404'}])
        +heading('Nothing Found','heading-sm', '')
        include ../components/not-found
        .default-slider(data-slider="default-slider")
          +default-slider('0')