extends ../layouts/master

block title
  title Swan Hellenic | Travel Agents

block basicSeo
  meta(content="This page Travel Agents" name="description")
  meta(content="Page, Travel Agents" name="keywords")

block manifestBrowserconfigFiles
  link(href="../manifest.json" rel="manifest" crossorigin)
  meta(content="../browserconfig.xml" name="msapplication-config")

block content
  main.section-not-green
    include ../mixins/breadcrumbs
    include ../mixins/heading
    section.gapper.section.section-white
      .wrapper
        +breadcrumbs([{type: 'span', title: 'Travel Agents'}])
        +heading('Travel Agents','heading-sm', '')
        include ../components/travel-agents